---
import { Image } from "astro:assets";
import sponsor from "../img/sponsor.svg";
import sponsor2 from "../img/sponsor-2.svg";
---

<p-slide>
  <Image
    style="border-radius: 1em;"
    src={sponsor}
    alt="Sponsor Logo"
    class="alter-image-size"
  />
</p-slide>
<p-slide>
  <Image
    style="border-radius: 1em;"
    src={sponsor2}
    alt="Sponsor Logo"
    class="alter-image-size"
  />
</p-slide>
